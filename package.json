{
  "name": "formalist-standard-react",
  "version": "0.0.1",
  "description": "A standard form implementation for Formalist using React",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "babel src --out-dir lib",
    "postbuild": "npm run copycss",
    "watch": "wr 'npm run build' ./src",
    "prepublish": "npm run build",
    "test": "NODE_ENV=test babel-node test | faucet",
    "posttest": "npm run lint",
    "clean": "rm -rf ./lib/*",
    "copycss": "rsync -r ./src/* --include='*/' --include='*.mcss' --exclude='*' lib",
    "lint": "eslint 'src/*.js' 'src/**/*.js' 'test/*.js' 'test/**/*.js'; exit 0",
    "watchlint": "wr 'npm run lint' ./src",
    "example": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/icelab/formalist-standard-react.git"
  },
  "keywords": [
    "formalist"
  ],
  "authors": [
    "Max Wheeler <max@icelab.com.au> (https://github.com/makenosound)",
    "Tim Riley <tim@icelab.com.au> (https://github.com/timriley)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/icelab/formalist-standard-react/issues"
  },
  "homepage": "https://github.com/icelab/formalist-standard-react",
  "devDependencies": {
    "@f/is-function": "^1.1.1",
    "autoprefixer": "^6.3.3",
    "babel-cli": "^6.6.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-css-modules-transform": "0.0.7",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.0.0",
    "eslint-config-standard-react": "^2.3.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-react": "^4.2.1",
    "eslint-plugin-standard": "^1.3.2",
    "eslint": "^2.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "faucet": "0.0.1",
    "jsdom": "^8.1.0",
    "loader-utils": "^0.2.12",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.1",
    "postcss-modules-extract-imports": "^1.0.0",
    "postcss-modules-local-by-default": "^1.0.1",
    "postcss-modules-scope": "^1.0.0",
    "postcss-modules-values": "^1.1.1",
    "postcss": "^5.0.19",
    "react-addons-test-utils": "^0.14.7",
    "style-loader": "^0.13.0",
    "tape": "^4.5.1",
    "webpack": "^1.12.14",
    "wr": "^1.3.1"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "formalist-compose": "icelab/formalist-compose",
    "is-number": "^2.1.0",
    "moment": "^2.12.0",
    "number-is-integer": "^1.0.0",
    "react-day-picker": "^1.3.1",
    "react-dnd-html5-backend": "2.1.2",
    "react-dnd": "2.1.2",
    "react-dom": "^0.14.7",
    "react-immutable-proptypes": "^1.7.0",
    "react-portal": "2.0.0",
    "react-textarea-autosize": "^3.3.0",
    "react": "^0.14.7",
    "scribe-editor": "^2.1.2",
    "shallow-equals": "0.0.0",
    "uid": "0.0.2"
  },
  "engines": {
    "node": ">=5.2.0"
  }
}
