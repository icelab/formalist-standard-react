{
  "name": "formalist-standard-react",
  "version": "3.0.4",
  "description": "A standard form implementation for Formalist using React",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "postbuild": "yarn run copycss && yarn run lint",
    "compile": "yarn run build",
    "precompile": "yarn run clean",
    "watch": "nodemon --watch src --exec 'yarn run build' --ext js,css,mcss",
    "prepublish": "yarn run compile",
    "test": "NODE_ENV=test babel-node test | faucet",
    "posttest": "yarn run lint",
    "clean": "rm -rf ./lib/*",
    "copycss": "rsync -r ./src/* --include='*/' --include='*.mcss' --include='*.css' --exclude='*' lib",
    "lint": "eslint 'src/*.js' 'src/**/*.js' 'test/*.js' 'test/**/*.js'; exit 0",
    "watchlint": "nodemon --watch src --exec 'yarn run lint'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/icelab/formalist-standard-react.git"
  },
  "keywords": [
    "formalist"
  ],
  "authors": [
    "Max Wheeler <max@icelab.com.au> (https://github.com/makenosound)",
    "Tim Riley <tim@icelab.com.au> (https://github.com/timriley)"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/icelab/formalist-standard-react/issues"
  },
  "homepage": "https://github.com/icelab/formalist-standard-react",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.9.0",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-css-modules-transform": "0.1.1",
    "babel-plugin-module-alias": "^1.6.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.16.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "blue-tape": "^0.2.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.7.1",
    "eslint-config-standard": "^6.2.0",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-promise": "3.0.0",
    "eslint-plugin-react": "6.3.0",
    "eslint-plugin-standard": "^2.0.1",
    "faucet": "0.0.1",
    "ignore-styles": "^1.2.0",
    "isfunction": "^0.0.6",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^8.3.1",
    "nodemon": "^1.9.2",
    "react-addons-test-utils": "15.3.2",
    "tape": "^4.5.1"
  },
  "dependencies": {
    "attache-upload": "^2.1.1",
    "classnames": "^2.2.5",
    "component-emitter": "1.2.1",
    "draft-js-ast-exporter": "^2.0.0",
    "draft-js-ast-importer": "^2.0.0",
    "draft-js-autolist-plugin": "^2.0.0",
    "draft-js-block-breakout-plugin": "^2.0.1",
    "draft-js-plugins-editor": "^2.0.0-rc3",
    "draft-js-single-line-plugin": "^2.0.1",
    "es6-promise": "^3.2.1",
    "formalist-compose": "^2.0.0",
    "formalist-data-object-renderer": "^1.0.0",
    "formalist-validation": "^1.0.0",
    "fuzzy": "0.1.1",
    "is-number": "^2.1.0",
    "lodash.assigninwith": "^4.0.7",
    "lodash.debounce": "4.0.7",
    "lodash.isequal": "^4.2.0",
    "lodash.partialright": "4.1.4",
    "moment": "^2.13.0",
    "number-is-integer": "^1.0.0",
    "postcss": "^5.0.21",
    "postcss-import": "^8.1.2",
    "postcss-modules-extract-imports": "^1.0.1",
    "postcss-modules-local-by-default": "^1.0.1",
    "postcss-modules-scope": "^1.0.1",
    "postcss-modules-values": "^1.1.3",
    "prop-types": "^15.5.8",
    "react-addons-update": "^15.6.2",
    "react-day-picker": "^6.1.0",
    "react-dnd": "^2.5.3",
    "react-dnd-html5-backend": "^2.5.3",
    "react-dropzone": "^4.1.3",
    "react-immutable-proptypes": "^2.1.0",
    "react-portal": "^3.1.0",
    "react-textarea-autosize": "^5.1.0",
    "shallow-equals": "0.0.0",
    "superagent": "^1.8.3",
    "trigger-event": "^1.0.3",
    "uid": "^0.0.2"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  }
}
