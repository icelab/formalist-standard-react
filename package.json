{
  "name": "formalist-standard-react",
  "version": "0.0.1",
  "description": "A standard form implementation for Formalist using React",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel src --out-dir lib",
    "postbuild": "npm run copycss",
    "compile": "npm run build",
    "precompile": "npm run clean",
    "watch": "wr 'npm run build' ./src",
    "prepublish": "npm run compile",
    "test": "NODE_ENV=test babel-node test | faucet",
    "posttest": "npm run lint",
    "clean": "rm -rf ./lib/*",
    "copycss": "rsync -r ./src/* --include='*/' --include='*.mcss' --exclude='*' lib",
    "lint": "eslint 'src/*.js' 'src/**/*.js' 'test/*.js' 'test/**/*.js'; exit 0",
    "watchlint": "wr 'npm run lint' ./src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/icelab/formalist-standard-react.git"
  },
  "keywords": [
    "formalist"
  ],
  "authors": [
    "Max Wheeler <max@icelab.com.au> (https://github.com/makenosound)",
    "Tim Riley <tim@icelab.com.au> (https://github.com/timriley)"
  ],
  "bugs": {
    "url": "https://github.com/icelab/formalist-standard-react/issues"
  },
  "homepage": "https://github.com/icelab/formalist-standard-react",
  "devDependencies": {
    "@f/is-function": "^1.1.1",
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.6.5",
    "babel-loader": "^6.2.4",
    "babel-plugin-css-modules-transform": "0.0.9",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "blue-tape": "^0.2.0",
    "enzyme": "^2.2.0",
    "eslint": "^2.7.0",
    "eslint-config-standard": "^5.1.0",
    "eslint-config-standard-react": "^2.3.0",
    "eslint-plugin-react": "^4.3.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-standard": "^1.3.2",
    "faucet": "0.0.1",
    "ignore-styles": "^1.2.0",
    "isomorphic-fetch": "^2.2.1",
    "jsdom": "^8.3.1",
    "react-addons-test-utils": "^15.0.1",
    "tape": "^4.5.1",
    "trigger-event": "^1.0.2",
    "wr": "^1.3.1"
  },
  "dependencies": {
    "attache-upload": "icelab/attache-upload.js",
    "bus": "component/bus",
    "classnames": "^2.2.3",
    "es6-promise": "^3.1.2",
    "formalist-compose": "icelab/formalist-compose",
    "formalist-validation": "icelab/formalist-validation",
    "fuzzy": "0.1.1",
    "is-number": "^2.1.0",
    "lodash.isequal": "^4.1.4",
    "moment": "^2.12.0",
    "number-is-integer": "^1.0.0",
    "postcss": "^5.0.19",
    "postcss-import": "^8.1.0",
    "postcss-modules-extract-imports": "^1.0.0",
    "postcss-modules-local-by-default": "^1.0.1",
    "postcss-modules-scope": "^1.0.0",
    "postcss-modules-values": "^1.1.2",
    "react-day-picker": "^1.3.2",
    "react-dnd": "2.1.4",
    "react-dnd-html5-backend": "2.1.2",
    "react-dropzone": "^3.3.2",
    "react-immutable-proptypes": "^1.7.1",
    "react-portal": "2.1.0",
    "react-textarea-autosize": "^4.0.0",
    "scribe-editor": "^3.0.0",
    "shallow-equals": "0.0.0",
    "superagent": "^1.8.1",
    "uid": "0.0.2"
  },
  "peerDependencies": {
    "react": "^15.0.0",
    "react-dom": "^15.0.0"
  },
  "license": "MIT"
}
